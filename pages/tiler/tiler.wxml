        </wux-upload>
      </van-tab>
      <van-tab disabled="{{tilerb_active_1}}" title="瓦工施工" custom-class="deg_cont">
        <wux-upload list-type="picture-card" file-list="{{ tilerb_fileList_1 }}" url="https://request.hejianzhiyang.com/Jinguanjia/uploadimg" bind:change="tilerb_onChange_1" bind:success="tilerb_onSuccess_1" bind:fail="tilerb_onFail_1" bind:complete="tilerb_onComplete_1" bind:preview="tilerb_onPreview_1" bind:remove="tilerb_onRemove_1">
            <wux-icon type='ios-add' size="50"/>
        </wux-upload>
      </van-tab>
    </van-tabs>
  </van-col>
</van-row>

<van-row custom-class="deg_title">
