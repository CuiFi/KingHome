<!--pages/editlist/editlist.wxml-->
<view class="weui-grids">
  <block wx:for="{{grids}}" wx:key="*_this">
    <navigator url="{{item.url}}" class="weui-grid" hover-class="weui-grid_active">
      <image class="weui-grid__icon" src="{{item.build_img}}" />
      <view class="weui-grid__label">{{item.build}}</view>
    </navigator>
  </block>
</view>
